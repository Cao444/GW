@model IEnumerable<GGW.Models.news>

@{
    ViewBag.Title = "Sel_news";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<link href="~/icon/css/bootstrap.min.css" rel="stylesheet" />
<script src="~/icon/js/bootstrap.min.js"></script>
<script src="~/icon/js/jquery.min.js"></script>

<div class="container-fluid">
    <div class="row" style="padding-top:10px;border-bottom:1px solid #e1e1e1">
        <a href="@Url.Action("Index","Admin")"><i class="glyphicon glyphicon-home"></i>&nbsp;首页</a><i style="color:blue">>>></i><a href="@Url.Action("Sel_news","Admin")"><i class=""></i>查看全部新闻</a>
    </div>
    <div class="row">
        <table id="sel_datatable" class="table table-hover">
            <tr>
                <th>标题</th>
                <th>上传时间</th>
                <th>内容</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.title</td>
                    <td>@item.uptime</td>
                    <td>@item.nr</td>
                </tr>
            }
        </table>
    </div>
</div>
